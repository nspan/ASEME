<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="SUC.presentation.SUCModelWizardID"
            name="%_UI_SUCModelWizard_label"
            class="SUC.presentation.SUCModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SUCModelFile.gif">
         <description>%_UI_SUCModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="SUC.presentation.SUCEditorID"
            name="%_UI_SUCEditor_label"
            icon="icons/full/obj16/SUCModelFile.gif"
            extensions="suc"
            class="SUC.presentation.SUCEditor"
            contributorClass="SUC.presentation.SUCActionBarContributor">
      </editor>
   </extension>

</plugin>
