   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="SAGdesign.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="SAG.diagram.navigator.SAGNavigatorContentProvider" 
            labelProvider="SAG.diagram.navigator.SAGNavigatorLabelProvider"
            icon="icons/obj16/SAGDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="sagd"/>
               </and>
               <instanceof value="SAG.diagram.navigator.SAGAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="SAG.diagram.navigator.SAGAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="SAGdesign.diagram.navigatorSorter" 
               class="SAG.diagram.navigator.SAGNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="sagd"/>
                  </and>
                  <instanceof value="SAG.diagram.navigator.SAGAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="SAGdesign.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="SAG.diagram.navigator.SAGDomainNavigatorContentProvider" 
            labelProvider="SAG.diagram.navigator.SAGDomainNavigatorLabelProvider"
            icon="icons/obj16/SAGDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="sag"/>
               </and>
               <instanceof value="SAG.diagram.navigator.SAGDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="SAG.diagram.navigator.SAGDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="SAGdesign.diagram.navigatorActionProvider"
            class="SAG.diagram.navigator.SAGNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="SAG.diagram.navigator.SAGAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
	   <extension point="org.eclipse.ui.navigator.linkHelper" id="navigator-linkhelper">
      <?gmfgen generated="true"?>
      <linkHelper
            id="SAGdesign.diagram.navigatorLinkHelper"
            class="SAG.diagram.navigator.SAGNavigatorLinkHelper">
         <editorInputEnablement>
            <and>
               <instanceof value="org.eclipse.emf.common.ui.URIEditorInput"/>
               <test property="SAGdesign.diagram.isURIEditorInput"/>
            </and>
         </editorInputEnablement>
         <selectionEnablement>
            <instanceof value="SAG.diagram.navigator.SAGAbstractNavigatorItem"/>
         </selectionEnablement>
      </linkHelper>
   </extension>
